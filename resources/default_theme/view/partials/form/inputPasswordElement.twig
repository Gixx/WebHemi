<div class="element input password{% if formElement.hasErrors() %} error{% endif %}">
    <label for="{{ formElement.getId() }}">{{ formElement.getLabel() }}</label>
    <input type="password" id="{{ formElement.getId() }}" name="{{ formElement.getName() }}"{{ _FORM.renderFormElementAttributes(formElement.getAttributes()) }} />
    {% if formElement.hasErrors() %}
        {% include '@WebHemi/partials/errorBlock.twig' with {'errors': formElement.getErrors()} %}
    {% endif %}
</div>
