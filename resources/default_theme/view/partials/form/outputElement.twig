<div class="element output{% if formElement.hasErrors() %} error{% endif %}">
    <label for="{{ formElement.getId() }}">{{ formElement.getLabel() }}</label>
    <output id="{{ formElement.getId() }}" name="{{ formElement.getName() }}"{{ _FORM.renderFormElementAttributes(formElement.getAttributes()) }}></output>
    {% if formElement.hasErrors() %}
        {% include '@WebHemi/partials/errorBlock.twig' with {'errors': formElement.getErrors()} %}
    {% endif %}
</div>
