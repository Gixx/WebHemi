{% import '@WebHemi/macros/renderForm.twig' as _FORM %}
<fieldset name="{{ formElement.getName() }}" id="{{ formElement.getId() }}" {{ _FORM.renderFormElementAttributes(formElement.getAttributes()) }}>
    {% set label = formElement.getLabel() %}
    {% if label is not empty %}
    <legend>{{ formElement.getLabel() }}</legend>
    {% endif %}
    {% if formElement.hasNodes() %}
        {{ _FORM.renderSubForm(formElement.getNodes()) }}
    {% endif %}
</fieldset>
