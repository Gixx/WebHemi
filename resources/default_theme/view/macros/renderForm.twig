{% macro renderForm(form) %}
    {% if defined('@Theme/partials/form/formElement.twig') %}
        {% include '@Theme/partials/form/formElement.twig' with {'form': form} %}
    {% else %}
        {% include '@WebHemi/partials/form/formElement.twig' with {'form': form} %}
    {% endif %}
{% endmacro %}

